<div class="filter-list hidden" data-view-element="listEl"></div>
<div class="filter-button-box" data-view-element="buttonBoxEl">
    <a class="button filter-button-create" data-view-element="createButtonEl">
        <span class="button-icon"></span>
        <span class="button-label">Create</span>
    </a>
    <a class="button filter-button-submit hidden" data-view-element="submitButtonEl">
        <span class="button-icon"></span>
        <span class="button-label">Submit</span>
    </a>
    <a class="button filter-button-clear hidden" data-view-element="clearButtonEl">
        <span class="button-icon"></span>
        <span class="button-label">Clear</span>
    </a>
</div>
